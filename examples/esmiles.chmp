compound ethane {
	esmiles(1, 0, "'H3C''CH3'");
};

compound propyne {
	esmiles(1, 0, "'H3C''C'#'CH'");
};

compound isobutane2 {
	esmiles(1, 0, "'H3C''CH2'('CH3')'CH3'");
};
compound isobutane3 {
	esmiles(1, 0, "'H3C''CH2'()('CH3')'CH3'");
};

compound acetone {
	esmiles(1, 0, "'H3C''C'(=O)'CH3'");
};

compound propanoic_acid {
	esmiles(1, 0, "'H3C''CH2'C()(=O)(O'H')");
};

compound propanoic_acid2 {
	coloresmiles(1, 0, "'H3C''CH2'C()(=O)(O'H')", blue, red);
};
